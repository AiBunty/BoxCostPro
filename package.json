{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx --env-file=.env server/index-dev.ts",
    "dev:node": "node --import tsx server/index-dev.ts",
    "verify:clerk": "npx tsx scripts/verify-clerk-env.ts",
    "auth:guard": "npx tsx scripts/auth-guard.ts",
    "test": "vitest --environment jsdom",
    "build": "npm run auth:guard && vite build && esbuild server/index-prod.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "validate:env": "node scripts/validate-env.js",
    "db:push": "drizzle-kit push",
    "replit:migrate": "bash scripts/replit-migrate.sh",
    "replit:migrate:ps": "powershell -ExecutionPolicy Bypass -File scripts/replit-migrate.ps1",
    "db:schema": "bash scripts/db-schema-dump.sh",
    "db:schema:ps": "powershell -ExecutionPolicy Bypass -File scripts/db-schema-dump.ps1",
    "local:db": "powershell -ExecutionPolicy Bypass -File scripts/local-db-setup.ps1 -UseDocker -SeedDefaults",
    "local:db:noseed": "powershell -ExecutionPolicy Bypass -File scripts/local-db-setup.ps1 -UseDocker",
    "verify:neon": "powershell -ExecutionPolicy Bypass -File scripts/neon-verify.ps1",
    "migrate:neon-to-local": "powershell -ExecutionPolicy Bypass -File scripts/migrate-neon-to-local.ps1 -AutoSwitch",
    "migrate:export": "npx tsx scripts/migration/export-neon-data.ts",
    "migrate:import": "npx tsx scripts/migration/import-local-data.ts",
    "migrate:validate": "npx tsx scripts/migration/validate-migration.ts",
    "migrate:switch": "powershell -ExecutionPolicy Bypass -File scripts/migration/switch-to-local-db.ps1",
    "migrate:rollback": "powershell -ExecutionPolicy Bypass -File scripts/migration/rollback-to-neon.ps1",
    "enforce:separation": "powershell -ExecutionPolicy Bypass -File scripts/enforce-separation.ps1",
    "lint:architecture": "npm run enforce:separation",
    "prebuild": "npm run enforce:separation"
  },
